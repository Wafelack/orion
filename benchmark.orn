(def ackermann (λ (m n)
                 (match (, m n)
                        ((, 0 _) (+ n 1))
                        ((, _ 0) (ackermann (- m 1) 1))
                        (_ (ackermann (- m 1) (ackermann m (- n 1)))))))

(def bench (λ (amount)
             (match amount
                    (0 (ackermann 3 3))
                    (_ (begin
                         (ackermann 3 3)
                         (bench (- amount 1)))))))
